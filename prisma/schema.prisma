generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}





model TaskPr {
  id Int @id @default(autoincrement())
  ygId String
  department String
  product String
  event String? 
  name String
  subdivision String
  tgId String
  branch String
  title String?
  description String?
  target String?
  build String?
  leader String?
  place String?
  site String?
  participants String?
  date  String?
  status String 
  createAt DateTime @default(now())
  updateAt DateTime @default(now())
}
