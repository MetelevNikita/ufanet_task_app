generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Task {
  id Int @id @default(autoincrement())
  ygId String
  department String
  fio String
  subdivision String
  tgId String
  branch String
  leader String
  type String
  title String
  deadline String
  message String
  status String
  stage String

}
